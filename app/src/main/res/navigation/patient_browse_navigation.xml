<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/patient_browse_navigation"
    app:startDestination="@id/doctorPatientFragment">
    <fragment
        android:id="@+id/doctorPatientFragment"
        android:name="com.compultra.hcare.ui.patient.doctor.DoctorPatientFragment"
        android:label="All Doctors"
        tools:layout="@layout/fragment_doctor_patient" />
    <fragment
        android:id="@+id/chatPatientFragment"
        android:name="com.compultra.hcare.ui.chat.ChatListFragment"
        android:label="Chats"
        tools:layout="@layout/fragment_chat_list" />
    <fragment
        android:id="@+id/prescriptionPatientFragment"
        android:name="com.compultra.hcare.ui.patient.prescription.PrescriptionPatientFragment"
        android:label="Prescriptions"
        tools:layout="@layout/fragment_prescription_patient" />
    <fragment
        android:id="@+id/consultationPatientFragment"
        android:name="com.compultra.hcare.ui.patient.consultation.ConsultationPatientFragment"
